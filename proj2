    "# Full connection\n",
    "classifier1.add(Dense(256,activation = 'relu',input_shape=(784,)))\n",
    "classifier1.add(Dense(10,activation = 'softmax'))\n",
    "# Compiling CNN\n",
    "classifier1.compile(optimizer = 'adam', loss = 'categorical_crossentropy', metrics=['accuracy'])\n",
    "classifier1.summary()"
